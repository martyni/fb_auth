          "parameter_depth": 1,
          "environment_variables": {
                  "env": "dev",
		  "static_host": "http://martyni-static.s3-website.eu-west-2.amazonaws.com",
		  "static_path": "/auth/dev/app"
          }		  
   },
  "stge": {
          "s3_bucket" : "martyni-lambda",
